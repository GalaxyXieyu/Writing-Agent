{
	"name": "writingagent-frontend",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint src --fix --ext .js,.jsx,.vue",
		"prettier": "prettier --write \"src/**/*.{vue,js,jsx}\"",
		"commit": "git-cz",
		"prepare": "husky install",
		"hooks": "husky add .husky/pre-commit \"npx --no-install lint-staged\"",
		"commit-msg": "husky add .husky/commit-msg \"npx --no-install commitlint --edit \"$1\"\"",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "./cz-config.cjs"
		}
	},
	"lint-staged": {
		"*.{js,vue}": [
			"pnpm run lint",
			"pnpm run prettier"
		]
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@mdi/js": "^7.4.47",
		"@microsoft/fetch-event-source": "^2.0.1",
		"@tiptap/core": "^2.10.2",
		"@tiptap/extension-bubble-menu": "^2.10.2",
		"@tiptap/starter-kit": "^2.10.2",
		"@tiptap/vue-3": "^2.10.2",
		"@types/file-saver": "^2.0.7",
		"@types/html-docx-js": "^0.3.4",
		"@vueuse/core": "^10.11.1",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"aieditor": "^1.4.2",
		"axios": "^1.7.2",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.10",
		"element-plus": "^2.7.6",
		"fuse.js": "^7.0.0",
		"html-docx-js": "^0.3.1",
		"interactjs": "^1.10.27",
		"marked": "^11.1.1",
    "html2pdf.js": "^0.10.1",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1",
		"marked-linkify-it": "^3.1.9",
		"markmap-common": "^0.16.0",
		"markmap-lib": "^0.16.1",
		"markmap-view": "^0.16.0",
		"microsoft-cognitiveservices-speech-sdk": "^1.36.0",
		"pinia": "2.0.36",
		"pinia-plugin-persistedstate": "^3.2.0",
		"prismjs": "^1.29.0",
		"qrcode": "^1.5.3",
		"recorder-core": "^1.3.23122400",
		"reka-ui": "^2.6.0",
		"simplebar": "^6.2.5",
		"simplebar-vue": "^2.3.3",
		"tailwind-merge": "^3.3.1",
		"uuid": "^9.0.1",
		"vue": "3.4.21",
		"vue-i18n": "9.14.5",
		"vue-router": "^4.4.5",
		"vue3-text-clamp": "^0.1.2",
		"vuetify": "3.0.4",
		"vuetify-pro-tiptap": "^2.5.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.2",
		"@commitlint/config-conventional": "^18.4.2",
		"@vitejs/plugin-vue": "^3.2.0",
		"@vue/runtime-core": "^3.3.8",
		"autoprefixer": "^10.4.21",
		"babel-plugin-transform-runtime": "^6.23.0",
		"commitizen": "^4.3.0",
		"commitlint-config-cz": "^0.13.3",
		"conventional-changelog-cli": "^4.1.0",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^7.0.0",
		"esbuild": "0.16.17",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-vue": "^9.18.1",
		"file-saver": "^2.0.5",
		"html-docx-js-typescript": "^0.1.5",
		"husky": "^8.0.3",
		"lint-staged": "^15.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.1.0",
		"sass": "^1.69.5",
		"sass-loader": "^13.3.2",
		"tailwindcss": "^3.4.18",
		"vite": "3.2.7",
		"vite-plugin-prismjs": "^0.0.11",
		"vite-plugin-svg-icons": "^2.0.1"
	}
}
